{
  "name": "@pizzajsdev/route-collector",
  "version": "0.9.0",
  "description": "A framework-agnostic file-based route collector inspired by Next.js's App Router",
  "author": "Javier Aguilar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pizzajsdev/pizza.git",
    "directory": "packages/route-collector"
  },
  "homepage": "https://github.com/pizzajsdev/pizza/tree/main/packages/route-collector#readme",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "src",
    "!src/**/*.test.ts"
  ],
  "type": "module",
  "exports": {
    "./fs": "./src/fs.ts",
    "./react-router": "./src/react-router.ts",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./fs": {
        "types": "./dist/fs.d.ts",
        "default": "./dist/fs.js"
      },
      "./react-router": {
        "types": "./dist/react-router.d.ts",
        "default": "./dist/react-router.js"
      },
      "./package.json": "./package.json"
    }
  },
  "devDependencies": {
    "@react-router/dev": "catalog:",
    "@types/node": "catalog:",
    "@typescript/native-preview": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "@react-router/dev": "catalog:"
  },
  "peerDependenciesMeta": {
    "@react-router/dev": {
      "optional": true
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsgo -p tsconfig.build.json",
    "clean": "git clean -fdX",
    "prepublishOnly": "pnpm run build",
    "test": "node --disable-warning=ExperimentalWarning --test './src/**/*.test.ts'",
    "typecheck": "tsgo --noEmit"
  },
  "keywords": [
    "router",
    "route-collector",
    "app-router",
    "nextjs",
    "react-router"
  ]
}
