{
  "name": "@pizzajsdev/react-router-hono",
  "version": "0.3.3",
  "description": "React Router v7+ adapter for Hono, compatible with Node and Vercel servers.",
  "homepage": "https://github.com/pizzajsdev/pizzajs#readme",
  "bugs": {
    "url": "https://github.com//pizzajsdev/pizzajs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pizzajsdev/pizzajs.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsup --clean",
    "postbuild": "publint",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "^1.18.1",
    "@vercel/nft": "^0.30.0",
    "esbuild": "^0.25.8",
    "hono": "^4.8.12",
    "minimatch": "^10.0.3",
    "vite": "^7.0.6"
  },
  "devDependencies": {
    "@react-router/dev": "^7.7.1",
    "@react-router/node": "^7.7.1",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "isbot": "^5.1.29",
    "publint": "^0.3.12",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.7.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
